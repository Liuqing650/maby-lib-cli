{
  "name": "maby-lib-cli",
  "version": "0.0.1",
  "description": "一个快速搭建 library 的基础环境。",
  "main": "lib/index.js",
  "browser": "lib/index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build && npm run gulp",
    "gulp": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liuqing650/maby-lib-cli.git"
  },
  "keywords": [
    "maby-lib-cli",
    "maby"
  ],
  "author": "liuqing",
  "license": "ISC",
  "files": [
    "dist/",
    "lib/",
    "CHANGELOG.md"
  ],
  "bugs": {
    "url": "https://github.com/Liuqing650/maby-lib-cli/issues"
  },
  "homepage": "https://github.com/Liuqing650/maby-lib-cli#readme",
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --progress --config webpack.config.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 10123
      }
    },
    "build": {
      "command": "webpack --config webpack.config.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 10125
      }
    },
    "analyzer": {
      "command": "webpack --json > stats.json --config webpack.config.js",
      "env": {
        "NODE_ENV": "production",
        "ANALYZER": true,
        "PORT": 3600
      }
    },
    "preview": {
      "command": "webpack --progress --config webpack.config.js",
      "env": {
        "NODE_ENV": "production",
        "ASSET_PATH": "./",
        "PREVIEW": true,
        "PORT": 3356
      }
    }
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.51",
