    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.17.4",
    "merge2": "^1.2.2",
    "postcss": "^6.0.23",
    "postcss-loader": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "rucksack-css": "^1.0.2",
    "start": "^5.1.0",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-webpack-plugin": "^0.10.5",
